{
  "expo": {
    "name": "AiLun Saúde",
    "slug": "ailun-saude",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00B4DB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ailun.saude",
      "buildNumber": "1.3.0",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "O aplicativo precisa acessar a câmera para consultas médicas por videochamada.",
        "NSMicrophoneUsageDescription": "O aplicativo precisa acessar o microfone para consultas médicas por videochamada.",
        "NSFaceIDUsageDescription": "Use Face ID para fazer login no aplicativo de forma rápida e segura."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00B4DB"
      },
      "package": "com.ailun.saude",
      "versionCode": 3,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "ailun-saude",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Use Face ID para fazer login no aplicativo."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "O aplicativo precisa acessar a câmera para consultas médicas."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "O aplicativo precisa acessar o microfone para consultas médicas."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#00B4DB",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ailun-saude-project-id"
      }
    }
  }
}